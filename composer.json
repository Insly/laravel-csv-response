{
    "name": "neoxia/laravel-csv-response",
    "license": "MIT",
    "description": "Add a CSV response type to Laravel",
    "require": {
        "php": "^8.0.2",
        "illuminate/routing": "^9.0||^11.0",
        "illuminate/support": "^9.0||^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6.12",
        "php-coveralls/php-coveralls": "^2.7"
    },
    "autoload": {
         "psr-4": {
            "Neoxia\\": "src/Neoxia/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neoxia\\Routing\\ResponseFactoryServiceProvider"
            ]
        }
    }
}
